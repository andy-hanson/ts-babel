{
  "scripts": {
    "build": "babel src -d lib --config-file ./babel.config.for_web",
    "build-watch": "babel src -d lib --config-file ./babel.config.for_web --watch",
    "build-for-flow": "babel src -d flow_checkable_src --keep-file-extension --config-file ./babel.config.for_flow",
    "build-for-flow-watch": "babel src -d flow_checkable_src --keep-file-extension --config-file ./babel.config.for_flow --watch",
    "check-flow": "flow",
    "check-flow-watch": "watch -t npm run check-flow",
    "check": "npm-run-all build-for-flow check-flow",
    "check-watch": "run-p build-for-flow-watch check-flow-watch",
    "check-and-build": "npm-run-all check build",
    "check-and-build-watch": "run-p check-watch build-watch"
  },
  "dependencies": {
    "regenerator-runtime": "^0.13.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.5.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-typescript": "^7.3.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/plugin-transform-typescript": "^7.6.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-flow": "^7.0.0",
    "flow-bin": "^0.108.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.6.3"
  }
}
